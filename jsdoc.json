{
  "recurseDepth": 10,
  "source": {
    "include": [
      "lib"
    ],
    "includePattern": ".js$",
    "excludePattern": "(./node_modules/|./docs/)_"
  },
  "sourceType": "module",
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "recurse": true,
    "template": "./node_modules/docdash",
    "verbose": true,
    "readme": "./README.md"
  }
}